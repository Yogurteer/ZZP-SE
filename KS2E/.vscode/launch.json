{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug main",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/main",
          "args": [],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "miDebuggerPath": "/usr/bin/gdb",
          "setupCommands": [
              {
                  "description": "启用 gdb 的漂亮打印功能",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ],
          "preLaunchTask": "make build"
      }
  ]
}
